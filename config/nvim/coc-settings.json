{
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 20,
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "typescript", "javascriptreact", "javascript.jsx"],
  "python.autoComplete.extraPaths": ["backend/src", "backend/tests/utils", "backend/tests"],
  "python.venvFolders": [
    ".virtualenvs",
    ".pyenv"
  ],
  "diagnostic.errorSign": "•",
  "diagnostic.warningSign": "•",
  "diagnostic.infoSign": "•",
  "python.jediEnabled": false,
  "python.linting.pep8Args": ["--max-line-length=160", "--ignore=E302,E402"],
  "prettier.eslintIntegration": true,
  "javascript.validate.enable": false
}

